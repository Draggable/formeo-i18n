{
  "name": "formeo-i18n",
  "version": "2.1.1",
  "description": "Language files for Formeo",
  "main": "dist/main.min.js",
  "sideEffects": false,
  "files": [
    "dist/*"
  ],
  "scripts": {
    "lint": "eslint src/ --ext .js || true",
    "generate:index": "node tools/generate-index.js",
    "build": "yarn generate:index && webpack --production",
    "semantic-release": "semantic-release --ci --debug",
    "test": "jest src/",
    "travis-deploy-once": "travis-deploy-once --pro",
    "translate": "node tools/translate.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Draggable/formeo-i18n.git"
  },
  "keywords": [
    "formeo",
    "lang",
    "files",
    "i18n"
  ],
  "author": "Kevin Chappell <kevin.b.chappell@gmail.com> (https://kevin-chappell.com)",
  "license": "MIT",
  "ignore": [
    "**/*",
    "node_modules"
  ],
  "bugs": {
    "url": "https://github.com/Draggable/formeo-i18n/issues"
  },
  "homepage": "https://github.com/Draggable/formeo-i18n#readme",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "*",
    "@babel/preset-env": "*",
    "@google-cloud/translate": "^2.1.2",
    "@semantic-release/changelog": "^3.0.1",
    "@semantic-release/git": "^7.0.5",
    "@semantic-release/npm": "^5.1.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "*",
    "babel-loader": "^8.0.4",
    "eslint": "^5.9.0",
    "eslint-config-google": "^0.11.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-jest": "^22.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "husky": "^1.1.4",
    "jest": "^23.6.0",
    "lint-staged": "^8.0.4",
    "lodash": "^4.17.11",
    "mi18n": "^0.4.8",
    "prettier": "^1.15.2",
    "semantic-release": "^15.12.0",
    "travis-deploy-once": "^5.0.9",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2"
  },
  "engines": {
    "node": ">=10"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "esmodules": true
          }
        }
      ]
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "printWidth": 120,
    "semi": false
  },
  "lint-staged": {
    "*.js": [
      "prettier --config package.json --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "plugins": [
      "jest",
      "prettier"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:jest/recommended",
      "google",
      "prettier"
    ],
    "env": {
      "es6": true,
      "node": true,
      "jest/globals": true
    },
    "parserOptions": {
      "ecmaVersion": 9,
      "sourceType": "module"
    },
    "rules": {
      "prefer-const": 1,
      "quote-props": 0,
      "no-unused-vars": 1,
      "no-console": 0,
      "new-cap": 0,
      "strict": 0,
      "arrow-parens": 0,
      "no-underscore-dangle": 0,
      "comma-dangle": 0,
      "no-use-before-define": 0,
      "eol-last": 0,
      "require-jsdoc": 1
    }
  },
  "jest": {
    "testRegex": "./.*\\.spec\\.js$"
  },
  "release": {
    "branch": "master",
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ],
    "prepare": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
