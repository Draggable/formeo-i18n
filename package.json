{
  "name": "formeo-i18n",
  "version": "0.0.1",
  "description": "Language files for Formeo",
  "main": "index.js",
  "scripts": {
    "lint": "eslint ./ --ext .js || true",
    "precommit": "lint-staged",
    "semantic-release": "semantic-release",
    "test": "jest ./",
    "travis-deploy-once": "travis-deploy-once"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Draggable/formeo-i18n.git"
  },
  "keywords": [
    "formeo",
    "lang",
    "files",
    "i18n"
  ],
  "author": "Kevin Chappell <kevin.b.chappell@gmail.com> (https://kevin-chappell.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Draggable/formeo-i18n/issues"
  },
  "homepage": "https://github.com/Draggable/formeo-i18n#readme",
  "dependencies": {},
  "devDependencies": {
    "@semantic-release/changelog": "^3.0.1",
    "@semantic-release/git": "^7.0.5",
    "@semantic-release/npm": "^5.1.1",
    "eslint": "^5.9.0",
    "eslint-config-google": "^0.11.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-prettier": "^3.0.0",
    "husky": "^1.1.4",
    "jest": "^23.6.0",
    "lint-staged": "^8.0.4",
    "mi18n": "^0.4.6",
    "prettier": "^1.15.2",
    "semantic-release": "^15.12.0"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false
  },
  "lint-staged": {
    "*.js": [
      "prettier --config package.json --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "plugins": [
      "prettier"
    ],
    "extends": [
      "eslint:recommended",
      "google",
      "prettier"
    ],
    "env": {
      "es6": true,
      "node": true
    },
    "rules": {
      "prefer-const": 1,
      "quote-props": 0,
      "no-unused-vars": 1,
      "no-console": 0,
      "new-cap": 0,
      "strict": 0,
      "arrow-parens": 0,
      "no-underscore-dangle": 0,
      "comma-dangle": 0,
      "no-use-before-define": 0,
      "eol-last": 0,
      "require-jsdoc": 1
    }
  },
  "jest": {
    "testRegex": "./.*\\.spec\\.js$"
  },
  "release": {
    "branch": "master",
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ],
    "prepare": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ]
  }
}
